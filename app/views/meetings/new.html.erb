<h1>Book a meeting</h1>

<%= simple_form_for([@room, @meeting]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs" data-provide='datepicker'>
  <%= f.input :name %>
    <hr>
    <%= f.input :start_time %>
    <hr>
    <%= f.datetime_select :end_time %>
    <hr>
  <div class="form-actions">
    <%= f.button :submit, "Book this meeting" %>
  </div>
<% end %>



