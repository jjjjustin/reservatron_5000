<h1>Index</h1>
this is the meeting index


<div class="container">
  <div class="row">
    <div class="col-md-3">
    </div>
    <div class="col-md-6">
      <div class="well well-lg">
        <h2 style="text-align: center">Scheduled Meetings  </h2>
      </div>
<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>Meeting</th>
      <th>Start Time</th>
      <th>End Time</th>
      <th>Scheduled by</th>
      <th>Location</th>
    </tr>
  </thead>
  <tbody>
  <% @meetings.each do |meeting| %>
    <tr>
      <td><%= meeting.name %></td>
      <td><%= meeting.start_timestamp %></td>
      <td><%= meeting.end_timestamp %></td>
      <td><%= meeting.room_id %></td>
      <td><%= link_to('Leave a Comment', new_meeting_comment_path(current_user, Comment.new), class: "btn btn-primary btn-lg") %></td>
       <td><%= link_to('Read comments', meeting_comments_path(@meetings), class: "btn btn-primary btn-lg") %></td>
      <% end %>
    </tr>
  </tbody>
</table>

    <div class="col-md-3">
    </div>
  </div>
</div>
